<?xml version="1.0" encoding="UTF-8"?>
<application
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/application_5.xsd"
        version="5">

    <description>Spark on GAE Microservices</description>
    <display-name>Spark Bank</display-name>

    <!-- Default Module -->
    <module>
        <web>
            <web-uri>bankapp-1.0-SNAPSHOT</web-uri>
            <context-root>bankapp</context-root>
        </web>
    </module>
    <!-- Other Modules -->
    <module>
        <web>
            <web-uri>mailservice-1.0-SNAPSHOT</web-uri>
            <context-root>mailservice</context-root>
        </web>
    </module>

    <module>
        <web>
            <web-uri>loggingservice-1.0-SNAPSHOT</web-uri>
            <context-root>loggingservice</context-root>
        </web>
    </module>

    <module>
        <web>
            <web-uri>transaction-listener-1.0-SNAPSHOT</web-uri>
            <context-root>transaction-listener</context-root>
        </web>
    </module>

    <library-directory>lib</library-directory>

</application>
